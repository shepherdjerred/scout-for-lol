{
  "name": "@scout-for-lol/report-ui",
  "version": "0.1.0",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "bun test || echo 'No tests found'",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:ci": "bun test --bail --coverage --reporter=junit --reporter-outfile=./junit.xml 2>&1 || echo 'No tests found - creating empty junit.xml' && echo '<?xml version=\"1.0\" encoding=\"UTF-8\"?><testsuites></testsuites>' > ./junit.xml",
    "lint": "bunx eslint src",
    "format": "bunx prettier --check src",
    "format:write": "bunx prettier --write src",
    "typecheck": "bunx tsc --noEmit"
  },
  "dependencies": {
    "@scout-for-lol/report": "workspace:*",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "bun-types": "latest",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "description": "Browser-compatible report UI for Scout for League of Legends",
  "keywords": [
    "league-of-legends",
    "report",
    "ui",
    "react",
    "vite"
  ],
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shepherdjerred/scout-for-lol.git"
  },
  "homepage": "https://github.com/shepherdjerred/scout-for-lol",
  "bugs": {
    "url": "https://github.com/shepherdjerred/scout-for-lol/issues"
  }
}
