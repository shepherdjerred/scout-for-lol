{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "lib": [
      "ES2021",
      "DOM",
      "DOM.Iterable"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "../..",
    "paths": {
      "@scout-for-lol/report-ui/*": [
        "./packages/report-ui/src/*"
      ],
      "@scout-for-lol/data/*": [
        "./packages/data/src/*"
      ],
      "@scout-for-lol/data": [
        "./packages/data/src/index.ts"
      ],
      "@scout-for-lol/report/*": [
        "./packages/report/src/*"
      ],
      "@scout-for-lol/report": [
        "./packages/report/src/index.ts"
      ],
      "@scout-for-lol/backend/*": [
        "./packages/backend/src/*",
        "./packages/backend/*"
      ]
    }
  },
  "include": [
    "src",
    "vite.config.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
