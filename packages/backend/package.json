{
  "name": "@scout-for-lol/backend",
  "version": "0.1.0",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "test": "bun test",
    "lint": "bunx eslint src",
    "format": "bunx prettier --check src",
    "format:write": "bunx prettier --write src",
    "typecheck": "bunx tsc --noEmit",
    "generate": "bun run db:generate",
    "db:generate": "bunx prisma generate",
    "db:push": "bunx prisma db push",
    "db:migrate": "bunx prisma migrate dev",
    "db:studio": "bunx prisma studio"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.890.0",
    "@prisma/client": "latest",
    "@resvg/resvg-js": "^2.6.2",
    "@scout-for-lol/data": "workspace:*",
    "@scout-for-lol/report": "workspace:*",
    "@sentry/node": "^10.0.0",
    "@typescript-eslint/utils": "^8.46.0",
    "cron": "^4.1.4",
    "date-fns": "^4.1.0",
    "discord.js": "^14.18.0",
    "dotenv": "^17.0.1",
    "env-var": "^7.5.0",
    "eslint-plugin-unicorn": "^61.0.2",
    "prisma": "latest",
    "prom-client": "^15.1.3",
    "react": "^19.1.0",
    "remeda": "^2.21.2",
    "satori": "^0.18.0",
    "ts-pattern": "^5.7.0",
    "twisted": "^1.63.3",
    "uuid": "^13.0.0",
    "yoga-wasm-web": "^0.3.3",
    "zod": "^4.0.0",
    "zod-validation-error": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^24.0.0",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "bun-types": "latest",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0"
  },
  "description": "Backend service for Scout for League of Legends Discord bot",
  "keywords": [
    "discord",
    "bot",
    "league-of-legends",
    "typescript",
    "bun"
  ],
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shepherdjerred/scout-for-lol.git"
  },
  "homepage": "https://github.com/shepherdjerred/scout-for-lol",
  "bugs": {
    "url": "https://github.com/shepherdjerred/scout-for-lol/issues"
  }
}
