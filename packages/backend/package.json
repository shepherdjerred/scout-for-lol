{
  "name": "@scout-for-lol/backend",
  "version": "0.1.0",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts --outdir=dist --target=node",
    "start": "bun run src/index.ts",
    "test": "bun test",
    "lint": "bunx eslint src",
    "format": "bunx prettier --check src",
    "format:write": "bunx prettier --write src",
    "typecheck": "bunx tsc --noEmit",
    "db:generate": "bunx prisma generate",
    "db:push": "bunx prisma db push",
    "db:migrate": "bunx prisma migrate dev",
    "db:studio": "bunx prisma studio",
    "generate": "bun run db:generate"
  },
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "@scout-for-lol/data": "workspace:*",
    "@sentry/node": "^8.0.0",
    "cron": "^3.0.0",
    "discord.js": "^14.0.0",
    "dotenv": "^16.0.0",
    "env-var": "^7.0.0",
    "prisma": "^6.0.0",
    "remeda": "^2.21.2",
    "ts-pattern": "^5.7.0",
    "twisted": "^1.66.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "bun-types": "latest",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.35.1"
  },
  "description": "Discord bot backend for Scout for LoL",
  "keywords": [
    "discord",
    "bot",
    "league-of-legends",
    "riot-api"
  ],
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shepherdjerred/scout-for-lol.git"
  },
  "homepage": "https://github.com/shepherdjerred/scout-for-lol",
  "bugs": {
    "url": "https://github.com/shepherdjerred/scout-for-lol/issues"
  }
}
