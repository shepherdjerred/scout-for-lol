{
  "name": "@scout-for-lol/backend",
  "version": "0.1.0",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:coverage:threshold": "bun test --coverage --bail",
    "test:ci": "bun test --bail --reporter=junit --reporter-outfile=./junit.xml",
    "test:fast": "bun test --bail=3",
    "test:debug": "bun test --randomize",
    "lint": "bunx eslint src",
    "format": "bunx prettier --check src",
    "format:write": "bunx prettier --write src",
    "typecheck": "bunx tsc --noEmit",
    "generate": "bun run db:generate",
    "db:generate": "bunx prisma generate && bun run brand-types && prettier --write generated/",
    "brand-types": "bun run scripts/brand-prisma-types.ts",
    "db:push": "bunx prisma db push",
    "db:migrate": "bunx prisma migrate dev",
    "db:studio": "bunx prisma studio",
    "test:review": "bun run src/league/review/test-reviews.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.890.0",
    "@prisma/client": "^6.0.0",
    "@resvg/resvg-js": "^2.6.2",
    "@scout-for-lol/data": "workspace:*",
    "@scout-for-lol/report": "workspace:*",
    "@scout-for-lol/report-ui": "workspace:*",
    "@sentry/node": "^10.0.0",
    "cron": "^4.1.4",
    "date-fns": "^4.1.0",
    "discord.js": "^14.18.0",
    "dotenv": "^17.0.1",
    "env-var": "^7.5.0",
    "openai": "^6.9.1",
    "prisma": "^6.0.0",
    "prom-client": "^15.1.3",
    "react": "^19.1.0",
    "remeda": "^2.21.2",
    "satori": "^0.18.0",
    "ts-pattern": "^5.7.0",
    "twisted": "^1.63.3",
    "uuid": "^13.0.0",
    "yoga-wasm-web": "^0.3.3",
    "zod": "^4.0.0",
    "zod-validation-error": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "@types/uuid": "^11.0.0",
    "aws-sdk-client-mock": "^4.1.0",
    "bun-types": "latest",
    "glob": "^11.0.3",
    "ts-morph": "^27.0.2",
    "typescript": "^5.0.0"
  },
  "description": "Backend service for Scout for League of Legends Discord bot",
  "keywords": [
    "discord",
    "bot",
    "league-of-legends",
    "typescript",
    "bun"
  ],
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shepherdjerred/scout-for-lol.git"
  },
  "homepage": "https://github.com/shepherdjerred/scout-for-lol",
  "bugs": {
    "url": "https://github.com/shepherdjerred/scout-for-lol/issues"
  }
}
