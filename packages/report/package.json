{
  "name": "@scout-for-lol/report",
  "version": "0.1.0",
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./browser": "./src/browser.ts",
    "./html/arena/report": "./src/html/arena/report.tsx",
    "./*": "./src/*"
  },
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:ci": "bun test --bail --coverage --reporter=junit --reporter-outfile=./junit.xml",
    "build": "bun run build.ts",
    "lint": "bunx eslint src",
    "format": "bunx prettier --check src",
    "format:write": "bunx prettier --write src",
    "typecheck": "bunx tsc --noEmit"
  },
  "dependencies": {
    "@resvg/resvg-js": "^2.6.2",
    "@scout-for-lol/data": "workspace:*",
    "react": "^19.1.1",
    "remeda": "^2.21.2",
    "satori": "^0.19.0",
    "ts-pattern": "^5.7.0",
    "yoga-wasm-web": "^0.3.3",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.13",
    "bun-types": "latest",
    "typescript": "^5.9.3"
  },
  "description": "Generate report images for League of Legends",
  "keywords": [
    "league-of-legends",
    "satori"
  ],
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shepherdjerred/scout-for-lol.git"
  },
  "homepage": "https://github.com/shepherdjerred/scout-for-lol",
  "bugs": {
    "url": "https://github.com/shepherdjerred/scout-for-lol/issues"
  }
}
