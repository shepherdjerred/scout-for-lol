    "format:write": "bunx prettier --write src",
    "typecheck": "bunx tsc --noEmit",
    "update-data-dragon": "bun run scripts/update-data-dragon.ts"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "remeda": "^2.21.2",
    "ts-pattern": "^5.7.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "bun-types": "latest",
    "typescript": "^5.9.3"
  }
}
