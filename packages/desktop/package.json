    "@tauri-apps/plugin-shell": "^2.2.2",
    "@tauri-apps/plugin-updater": "^2.2.0",
    "@tauri-apps/plugin-dialog": "^2.2.0",
    "@tauri-apps/plugin-fs": "^2.2.0",
    "discord.js": "^14.18.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zod": "^4.0.0",
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.4.0",
    "@types/node": "^24.0.0",
