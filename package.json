{
  "name": "scout-for-lol",
  "private": true,
  "workspaces": [
    "packages/backend",
    "packages/data",
    "packages/report",
    "packages/frontend"
  ],
  "scripts": {
    "install:all": "bun install",
    "typecheck:all": "bun run --filter='*' typecheck",
    "lint:all": "bun run --filter='*' lint",
    "format:all": "bun run --filter='*' format",
    "test:all": "bun run --filter='*' test",
    "clean": "rm -rf node_modules packages/*/node_modules",
    "generate": "bun run --filter='*' generate"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.890.0"
  }
}
